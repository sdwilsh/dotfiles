{
    "editor.fontFamily": "'FiraCode Nerd Font Mono',  monospace",
    "remote.SSH.path": "/usr/bin/ssh",
    "security.promptForRemoteFileProtocolHandling": false,
    "dev.containers.defaultFeatures": {
        "ghcr.io/devcontainers-extra/features/gh-release:1": {
            "assetRegex": ".*(\\.tar\\.gz)$",
            "binaryNames": "atuin",
            "repo": "atuinsh/atuin"
        },
        "ghcr.io/devcontainers-extra/features/starship:1": {
            "version": "latest"
        },
        "ghcr.io/meaningful-ooo/devcontainer-features/fish:2": {}
    },
    "dotfiles.repository": "https://github.com/sdwilsh/dotfiles.git",
    "dotfiles.targetPath": "~/.local/share/chezmoi",
    "nrf-connect.enableTelemetry": true,
    "terminal.integrated.defaultProfile.linux": "fish",
    "terminal.integrated.defaultLocation": "editor",
    "terminal.integrated.ignoreProcessNames": [
        "starship",
        "oh-my-posh",
        "bash",
        "zsh",
        "fish"
    ],
    "terminal.integrated.suggest.enabled": false
}

